<div class="row">
<div class="col">
    <h1>Editing Wiki</h1>
</div>
<div class="col">
    <%= link_to 'Back', wikis_path, class: 'btn btn-warning float-right mt-2' %>
</div>
</div>
<%= render 'form', wiki: @wiki %>
<% if current_user.admin? || (@wiki.user == current_user && current_user.premium?) %>
    <%= render partial: "collaborators/collaborators", locals: { wiki: @wiki, users: @users } %>
<% end %>
